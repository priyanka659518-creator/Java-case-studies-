Series Generator (Fibonacci)
import java.util.Scanner;

class FibonacciPriyanka {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter terms Priyanka: ");
        int n = sc.nextInt();

        int a = 0, b = 1;

        System.out.print("Priyanka Series: ");
        for(int i=1; i<=n; i++){
            System.out.print(a + " ");
            int c = a + b;
            a = b;
            b = c;
        }
    }
}
