Reverse Processing Unit (Reverse number)
import java.util.Scanner;

class ReversePriyanka {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter number Priyanka: ");
        int num = sc.nextInt();

        int rev = 0;
        while(num > 0){
            rev = rev * 10 + (num % 10);
            num = num / 10;
        }

        System.out.println("Priyanka Reversed = " + rev);
    }
}
